<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="build.dream.catering.mappers.PackageGroupGoodsMapper">
    <insert id="insertAll" parameterType="java.util.List" useGeneratedKeys="true" keyProperty="id">
        <bind name="sql" value="@build.dream.common.utils.BeanUtils@generateInsertAllSql('build.dream.common.erp.catering.domains.PackageGroupGoods')"></bind>
        ${sql[0]}
        <foreach collection="list" index="index" item="item" separator=",">
            ${sql[1]}
        </foreach>
    </insert>

    <delete id="deleteAll" parameterType="build.dream.common.utils.SearchModel">
        DELETE FROM package_group_goods <include refid="build.dream.catering.mappers.CommonMapper.searchConditionSql"></include>
    </delete>
</mapper>