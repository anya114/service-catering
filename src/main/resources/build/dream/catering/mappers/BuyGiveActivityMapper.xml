<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="build.dream.catering.mappers.BuyGiveActivityMapper">
    <insert id="insert" parameterType="build.dream.common.erp.catering.domains.BuyGiveActivity" useGeneratedKeys="true" keyProperty="id">
        ${@build.dream.common.utils.BeanUtils@generateInsertSql("build.dream.common.erp.catering.domains.BuyGiveActivity")}
    </insert>

    <insert id="insertAll" parameterType="java.util.List" useGeneratedKeys="true" keyProperty="id">
        <bind name="sql" value="@build.dream.common.utils.BeanUtils@generateInsertAllSql('build.dream.common.erp.catering.domains.BuyGiveActivity')"></bind>
        ${sql[0]}
        <foreach collection="list" index="index" item="item" separator=",">
            ${sql[1]}
        </foreach>
    </insert>

    <select id="findAll" parameterType="build.dream.common.utils.SearchModel" resultType="build.dream.common.erp.catering.domains.BuyGiveActivity">
        SELECT * FROM buy_give_activity <include refid="build.dream.catering.mappers.CommonMapper.searchConditionSql"></include>
    </select>
</mapper>